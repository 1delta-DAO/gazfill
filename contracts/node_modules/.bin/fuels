#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/achthar/gazfill/node_modules/.pnpm/fuels@0.93.0/node_modules/fuels/node_modules:/home/achthar/gazfill/node_modules/.pnpm/fuels@0.93.0/node_modules:/home/achthar/gazfill/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/achthar/gazfill/node_modules/.pnpm/fuels@0.93.0/node_modules/fuels/node_modules:/home/achthar/gazfill/node_modules/.pnpm/fuels@0.93.0/node_modules:/home/achthar/gazfill/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../fuels/fuels.js" "$@"
else
  exec node  "$basedir/../fuels/fuels.js" "$@"
fi
